<?xml version="1.0" encoding="UTF-8"?>
<!-- xml문서의 유효성 체크를 위해 필요한 부분 -->
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
  <configuration>
  
      <typeAliases>
          <typeAlias type="com.kh.hello.admin.model.vo.Question" alias="Question"/>
          <typeAlias type="com.kh.hello.admin.model.vo.Blacklist" alias="Blacklist"/>
          <typeAlias type="com.kh.hello.admin.model.vo.Report" alias="Report"/>
          <typeAlias type="com.kh.hello.message.model.vo.Message" alias="Message"/>
          <typeAlias type="com.kh.hello.admin.model.vo.DatePick" alias="DatePick"/>
          <typeAlias type="com.kh.hello.admin.model.vo.Reservation" alias="Reservation"/>
          <typeAlias type="com.kh.hello.member.model.vo.Member" alias="Member"/>
          <typeAlias type="com.kh.hello.seller.model.vo.Company" alias="Company"/>
          <typeAlias type="com.kh.hello.seller.model.vo.Room" alias="Room"/>
          <typeAlias type="com.kh.hello.seller.model.vo.RegistrationHistory" alias="RH"/>
          <typeAlias type="com.kh.hello.board.model.vo.Attachment" alias="Attachment"/>
          <typeAlias type="com.kh.hello.seller.model.vo.Attachment" alias="SellerAT"/>
          <typeAlias type="com.kh.hello.seller.model.vo.SearchProduct" alias="Product"/>
      </typeAliases>
  
      <mappers>
          <mapper resource="mappers/question-mapper.xml"/>
          <mapper resource="mappers/blacklist-mapper.xml"/>
          <mapper resource="mappers/message-mapper.xml"/>
          <mapper resource="mappers/report-mapper.xml"/>
          <mapper resource="mappers/reservation-mapper.xml"/>
          <mapper resource="mappers/member-mapper.xml"/>
          <mapper resource="mappers/company-mapper.xml"/>
          <mapper resource="mappers/room-mapper.xml"/>
          <mapper resource="mappers/rh-mapper.xml"/>
          <mapper resource="mappers/attachment-mapper.xml"/>
          <mapper resource="mappers/sellerAT-mapper.xml"/>
          <mapper resource="mappers/product-mapper.xml"/>
      </mappers>
  
  </configuration>